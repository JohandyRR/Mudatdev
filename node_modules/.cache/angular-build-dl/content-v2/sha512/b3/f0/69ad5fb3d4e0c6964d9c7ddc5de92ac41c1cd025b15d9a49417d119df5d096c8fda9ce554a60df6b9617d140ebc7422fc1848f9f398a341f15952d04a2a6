let ProfilePage = class ProfilePage {
    constructor(database, authservice) {
        this.database = database;
        this.authservice = authservice;
        firebase_app__WEBPACK_IMPORTED_MODULE_3___default.a.auth().onAuthStateChanged(user => {
            console.log("AUTH_USER", user);
            if (user) {
                const result = this.database.doc(`/profile/${this.authservice.getUID()}`);
                var userprofile = result.valueChanges();
                userprofile.subscribe(profile => {
                    console.log("PROFILE::", profile);
                    this.profileName = profile['name'];
                    this.profileImageUrl = profile['photoUrl'];
                    this.profileEmail = profile['email'];
                });
            }
        });
    }
    ngOnInit() {
    }
};
